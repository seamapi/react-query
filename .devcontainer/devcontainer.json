{
  "name": "TypeScript",
  "build": {
    "dockerfile": "Dockerfile",
    "args": {
      "VARIANT": "22"
    }
  },
  "extensions": [
    "ms-vsliveshare.vsliveshare",
    "dbaeumer.vscode-eslint",
    "EditorConfig.EditorConfig",
    "esbenp.prettier-vscode"
  ],
  "forwardPorts": [8080],
  "portsAttributes": {
    "8080": { "label": "Examples" }
  },
  "postCreateCommand": "npm install",
  "remoteUser": "node"
}
